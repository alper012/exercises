import java.util.*;

class Main {

    public static void main (String[]args){
        String sentence = "WelCoMe To jAVa";
        
        for(int i=0; i<sentence.length();i++){
            char c = sentence.charAt(i);
            if(Character.isUpperCase(c)){
                System.out.println(c);
            }
        }
        }
    }




